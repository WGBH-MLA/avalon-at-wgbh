I"(function() {
  var button_form, section_form;

  section_form = $('#associated_files form');

  button_form = $('#workflow_buttons form');

  $('input[type=text]', section_form).each(function() {
    return $(this).change(function() {
      var double, double_id;
      double_id = ($(this).attr('id')) + "_double'";
      double = $("input[id='" + double_id + "']", button_form);
      if (!(double.length > 0)) {
        double = $("<input type='hidden' id='" + double_id + "' name='" + ($(this).attr('name')) + "' value='" + ($(this).val()) + "'/>").appendTo(button_form);
      }
      return double.val($(this).val());
    });
  });

  $('input[type=submit]', section_form).hide();

  $('.date-input').datepicker({
    dateFormat: 'yy-mm-dd'
  });

}).call(this);
